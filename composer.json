{
    "name": "habib/laravel-translation-manager",
    "description": "A WebUI to manage translation files for Laravel, create or update existing languages files.",
    "keywords": ["laravel", "translation", "translation manager"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6|^7",
        "illuminate/translation": "^5.5|^6|^7",
        "league/flysystem": "^1.0",
        "Illuminate/http": "^5.5|^6|^7",
        "illuminate/routing": "^5.5|^6|^7"
    },
    "autoload": {
        "psr-4": {
            "Habib\\TranslationManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohamed Habib",
            "email": "mohamedhabibwork@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Habib\\TranslationManager\\TranslationManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
